{
		"API": "data/mmt.json",
		"mapping": {
			"results": {
				"source": "hotelsList",
				"format": {
					"name": {
						"source": "name"
					},
					"image": {
						"source": "mainImgUrl"
					},
					"rating": {
						"source": "htlAvgRating"
					},
					"users": {
						"source": "userReviewsCount"
					},
					"place": {
						"source": "line1"
					},
					"latitude": {
						"source": {
							"geoLocation": "latitude"
						}
					},
					"longitude": {
						"source": {
							"geoLocation": "longitude"
						}
					}
				}
			}
		}
	}, {
		"API": "data/xpedia.json",
		"mapping": {
			"results": {
				"source": "results",
				"format": {
					"name": {
						"source": "hotelName"
					},
					"image": {
						"source": "largeThumbnailURL"
					},
					"rating": {
						"source": "hotelStarRating"
					},
					"users": {
						"source": "reviewTotal"
					},
					"place": {
						"source": "line1"
					},
					"latitude": {
						"source": "latitude"
					},
					"longitude": {
						"source": "longitude"
					}
				}
			}
		}
	}